{
  "name": "svg2geo",
  "version": "1.0.0",
  "description": "Convert SVG to GeoJSON",
  "main": "./lib/index.js",
  "bin": {
    "svg2geo": "./lib/cli.js"
  },
  "scripts": {
    "build": "rm -rf lib && tsc && chmod u+x ./lib/cli.js",
    "dev": "ts-node ./src/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint tslint.json",
    "test": "jest --verbose --watchAll --coverage",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "homepage": "https://github.com/W2eK/svg2geo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/W2eK/svg2geo.git"
  },
  "keywords": [
    "cli",
    "svg",
    "geojson"
  ],
  "author": "Vladimir Osokin <osokin.home@gmail.com>",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@turf/helpers": "^6.5.0",
    "@turf/projection": "^6.5.0",
    "@turf/rewind": "^6.5.0",
    "@turf/truncate": "^6.5.0",
    "svg-path-parser": "^1.1.0",
    "svg-path-properties": "^1.0.9",
    "svg-points": "^6.0.1",
    "svgo": "^2.3.1",
    "xmldom": "^0.3.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/svg-path-parser": "^1.1.2",
    "@types/svgo": "^2.3.1",
    "@types/xmldom": "^0.1.31",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.31.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
