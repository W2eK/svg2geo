{
  "name": "svg2geo",
  "version": "1.0.0",
  "description": "Convert SVG to GeoJSON",
  "main": "./lib/index.js",
  "bin": "./lib/index.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "dev": "ts-node ./src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint tsconfig.json",
    "test": "jest --verbose --watchAll --coverage"
  },
  "homepage": "https://github.com/W2eK/svg2geo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/W2eK/svg2geo.git"
  },
  "keywords": [
    "cli",
    "svg",
    "geojson"
  ],
  "author": "Vladimir Osokin <osokin.home@gmail.com>",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@turf/helpers": "^6.5.0",
    "@turf/projection": "^6.5.0",
    "@turf/rewind": "^6.5.0",
    "@turf/truncate": "^6.5.0",
    "abs-svg-path": "^0.1.1",
    "parse-svg-path": "^0.1.2",
    "svg-path-properties": "^1.0.9",
    "svg-points": "^6.0.1",
    "svgo": "^2.3.1",
    "xmldom": "^0.3.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/svgo": "^2.3.1",
    "@types/xmldom": "^0.1.31",
    "@types/yargs": "^17.0.2",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
